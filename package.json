{
  "name": "lensshare",
  "version": "2.0.0-beta",
  "private": true,
  "license": "AGPL-3.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --parallel",
    "codegen": "turbo run codegen --parallel",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint --parallel",
    "lint:affected": "turbo run lint --parallel --filter=[origin/main]",
    "lint:fix": "turbo run lint:fix --parallel",
    "prepare": "husky install",
    "prettier": "turbo run prettier --parallel",
    "prettier:affected": "turbo run prettier --parallel --filter=[origin/main]",
    "prettier:fix": "turbo run prettier:fix --parallel",
    "start": "turbo run start --parallel",
    "test:dev": "turbo run test:dev",
    "test:e2e": "start-server-and-test start '8083|8086|8087|8090' test:dev",
    "typecheck": "turbo run typecheck --parallel",
    "typecheck:affected": "turbo run typecheck --parallel --filter=[origin/main]",
    "workers:deploy": "turbo run worker:deploy --parallel"
  },
  "eslintConfig": {
    "root": true
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "start-server-and-test": "^2.0.2",
    "turbo": "^1.10.16"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=16.18.1"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@cfworker/base64url": "^1.12.5",
    "@cloudflare/workers-types": "^4.20231025.0",
    "@growthbook/growthbook": "^0.30.0",
    "@huddle01/react": "^0.0.17",
    "@vercel/analytics": "^1.1.1",
    "@web3modal/wagmi": "^3.2.1",
    "@xmtp/react-sdk": "^3.0.0",
    "@xmtp/xmtp-js": "^11.2.1",
    "dayjs": "^1.11.10",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.7",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "graphql": "^16.8.1",
    "hooks": "link:@huddle01/react/hooks",
    "idb-keyval": "^6.2.1",
    "itty-router": "^4.0.23",
    "linkedom": "^0.16.4",
    "million": "^2.6.4",
    "next": "^14.0.1",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "request-ip": "^3.3.0",
    "tlds": "^1.245.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "xmtp-content-type-remote-attachment": "^1.0.7"
  }
}
